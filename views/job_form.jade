extends layout

block content
	form(action='', method='post')
		input(
			type='text',
			name='job_name',
			placeholder='Enter Job Name',
			required
		)
		input(
			type='number',
			name='grace',
			placeholder='Enter grace time',
			required
		)
		input(
			type='number',
			name='limit',
			placeholder='Enter limit time',
			required
		)
		input(
			type='submit',
			value='Submit'
		)