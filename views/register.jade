extends layout

block content
	form(action='', method='post')
		input(
			type='text',
			placeholder='First Name',
			name='fname',
			required
		)
		input(
			type='text',
			placeholder='Last Name',
			name='lname',
			required
		)
		input(
			type='email',
			placeholder='Enter your Email',
			name='email',
			required
		)
		input(
			type='password',
			placeholder='Enter your password',
			name='password',
			required
		)
		input(
			type='submit',
			value='Register'
		)
